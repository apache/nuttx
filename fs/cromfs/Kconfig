config FS_CROMFS
	bool "CROMFS file system"
	default n
	depends on !DISABLE_MOUNTPOINT
	select LIBC_LZF
	---help---
		Enable Compessed Read-Only Filesystem (CROMFS) support

if FS_CROMFS
endif
