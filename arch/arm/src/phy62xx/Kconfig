comment "PHY62xx Configuration Options"

choice
	prompt "only PHY 6222 Chip Selection"
	default ARCH_CHIP_PHY6222
	depends on ARCH_CHIP_PHY62XX

config ARCH_CHIP_PHY6222
	bool "PHY6222"
endchoise

config PHY6222_BLE
	bool "enable ble"
	default n

config PHYPLUS_STUB
	bool "enable phyplus stub"
	default y

