menuconfig VNCCLIENT
	bool "VNC client"
	default n
	depends on NET_TCP && !VNC_SERVER && EXPERIMENTAL
	---help---
		Enable support for a VNC Remote Frame Buffer (RFB) client

if VNCCLIENT

endif # VNCCLIENT
