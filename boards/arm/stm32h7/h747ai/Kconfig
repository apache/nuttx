if ARCH_BOARD_H747AI

choice
	prompt "Nucleo H747AI SMPS/LDO configuration"
	default H747AI_SMPS

config H747AI_SMPS
	bool "Internal SMPS only (default)"
	select STM32H7_PWR_DIRECT_SMPS_SUPPLY

config H747AI_LDO
	bool "Internal LDO only"

config H747AI_SMPS_LDO
	bool "Internal SMPS and LDO cascaded (not supported)"

config H747AI_BYPASS
	bool "Bypass (not supported)"

endchoice

endif # ARCH_BOARD_H747AI
